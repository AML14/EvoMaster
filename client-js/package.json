{
  "name": "evomaster-client-js",
  "version": "0.4.1",
  "description": "Client library for EvoMaster for JavaScript",
  "repository": {},
  "author": "EvoMaster Team",
  "license": "LGPL v3",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "test": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "tests/.*-test\\.(js|jsx|ts|tsx)$",
    "collectCoverageFrom": [
      "src/**.(js|jsx|ts|tsx)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  }
}
