    "@babel/types": "^7.7.2",
    "@types/babel__traverse": "^7.0.7",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/superagent": "^4.1.4",
    "@types/supertest": "^2.0.8",
    "dedent": "^0.7.0",
    "jest": "^24.9.0",
    "superagent": "^5.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "scripts": {
